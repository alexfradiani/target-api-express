{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["prettier", "prettier/@typescript-eslint", "google"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"]
  }
}
